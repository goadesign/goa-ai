{
  "tools": [
    {
      "confirmation": {
        "denied_result_template": "{\"summary\":\"Cancelled\",\"key\":\"{{ .Key }}\"}",
        "prompt_template": "Approve write: set {{ .Key }} to {{ .Value }}",
        "title": "Confirm change"
      },
      "description": "Write a stateful change",
      "id": "atlas.commands.dangerous_write",
      "payload": {
        "name": "DangerousWritePayload",
        "schema": {
          "$schema": "https://json-schema.org/draft/2020-12/schema",
          "example": {
            "key": "Est aut recusandae enim fuga.",
            "value": "Commodi molestias dignissimos."
          },
          "properties": {
            "key": {
              "description": "Key to update.",
              "example": "Aut vel consectetur ducimus minus.",
              "type": "string"
            },
            "value": {
              "description": "New value.",
              "example": "Occaecati non aliquam et laudantium.",
              "type": "string"
            }
          },
          "required": [
            "key",
            "value"
          ],
          "title": "DangerousWriteArgs",
          "type": "object"
        }
      },
      "result": {
        "name": "DangerousWriteResult",
        "schema": {
          "$schema": "https://json-schema.org/draft/2020-12/schema",
          "example": {
            "key": "Iure consequatur quia ex odit animi dolorum.",
            "summary": "Blanditiis sed quae."
          },
          "properties": {
            "key": {
              "description": "Key that was updated.",
              "example": "Rerum laudantium enim perspiciatis est vitae.",
              "type": "string"
            },
            "summary": {
              "description": "Summary of the update.",
              "example": "Nulla saepe qui iure qui.",
              "type": "string"
            }
          },
          "required": [
            "summary",
            "key"
          ],
          "title": "DangerousWriteResult",
          "type": "object"
        }
      },
      "service": "alpha",
      "title": "Dangerous Write",
      "toolset": "alpha.atlas.commands"
    }
  ]
}
